<h3>This week Interview Schedule</h3>
<% unless this_week_interview_schedule.blank?%>
<table id="box-table-a">
<tr>
  <th>Name</th>
  <th>Scheduled at</th>
  <th>Interviewer</th>
  <th>Actions</th>
</tr>
<% this_week_interview_schedule.each do |interview| %>
<tr>
  <td><%= interview.candidate.name %></td>
  <td><%= interview.scheduled_at.to_s(:simple)%> at <%= interview.schedule_time%></td>
  <td><%= User.find(interview.user_id).name %></td>
  <td><%= feedback_link(interview)%></td>
</tr>
<% end %>
</table>
<%else%>
<p class="no_interview_schedule">No Interview Schedule for this week</p>
<%end%>